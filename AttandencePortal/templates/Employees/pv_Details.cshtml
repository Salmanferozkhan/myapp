<table class="table table-hover table-bordered" style="width:80%;">
    <tr>
        <td>Eid:</td>
        <td>
            <div class="col-md-5">
                <input type="text" class="form-control" ng-disabled="true" ng-model="Emp.Eid" />
            </div>
        </td>
    </tr>
    <tr>
        <td>EName:</td>
        <td>
            <div class="col-md-5">
                <input type="text" class="form-control" ng-model="Emp.EName" />
            </div>
        </td>
    </tr>
    <tr>
        <td>ESalary:</td>
        <td>
            <div class="col-md-5">
                <input type="number" class="form-control" ng-model="Emp.ESalary" />
            </div>
        </td>
    </tr>
    <tr>
        <td>EGender:</td>
        <td>
            <input type="radio" value="Male" ng-model="Emp.EGender" /> Male
            <input type="radio" value="Female" ng-model="Emp.EGender" /> Female
        </td>
    </tr>
    <tr>
        <td>Date OF Joining:</td>
        <td>
            <div class="col-md-5">
                <input type="date" class="form-control" value="" ng-model="Emp.DateOfJoining" />
            </div>
        </td>
    </tr>
    <tr>
        <td>Did:</td>
        <td>
            <div class="col-md-5">
                <input type="text" class="form-control" value="" ng-model="Emp.Did" />
            </div>
        </td>
    </tr>
    <tr>
        <td><input type="button" class="btn btn-danger" ng-click="Create(Emp)" value="Create" /></td>
        <td><input type="button" class="btn btn-info" ng-click="UpdateEmployee(Emp)" value="Update" /></td>
    </tr>
    <tr>
        <td colspan="2">{{Msg}}</td>
    </tr>
</table>
